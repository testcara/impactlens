# Project configuration (non-sensitive settings)
# These settings can be safely version-controlled
project:
  # GitHub repository settings
  github_repo_owner: "konflux-ci"  # GitHub repository owner/organization
  github_repo_name: "konflux-ui"  # GitHub repository name

  # Google Sheets settings (optional)
  google_spreadsheet_id: ""  # Google Spreadsheet ID for report upload

# Output directory for reports (optional, for multi-team isolation)
# If not specified, defaults to "reports/github"
# Example for team-specific isolation: "reports/team-a/github"
# output_dir: "reports/team-a/github"

phases:
  - name: "No AI Period"
    start: "2024-10-24"
    end: "2025-05-31"
  - name: "Cursor Period"
    start: "2025-06-01"
    end: "2025-07-31"
  - name: "Full AI Period"
    start: "2025-08-01"
    end: "2025-11-08"

